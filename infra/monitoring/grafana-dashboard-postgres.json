{
  "title": "QatarCash \u2014 Postgres Overview",
  "panels": [
    {
      "type": "graph",
      "title": "Connections",
      "targets": [
        {
          "expr": "sum(pg_stat_activity_count)"
        }
      ]
    },
    {
      "type": "graph",
      "title": "TPS (commits+rollbacks)",
      "targets": [
        {
          "expr": "rate(pg_stat_database_xact_commit[5m]) + rate(pg_stat_database_xact_rollback[5m])"
        }
      ]
    },
    {
      "type": "graph",
      "title": "Buffer Cache Hit Ratio (%)",
      "targets": [
        {
          "expr": "100 * (1 - (rate(pg_stat_database_blks_read[5m]) / (rate(pg_stat_database_blks_read[5m]) + rate(pg_stat_database_blks_hit[5m]))))"
        }
      ]
    },
    {
      "type": "graph",
      "title": "Locks",
      "targets": [
        {
          "expr": "sum(pg_locks_count)"
        }
      ]
    }
  ],
  "schemaVersion": 36
}
