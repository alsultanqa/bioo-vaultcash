{
  "title": "QatarCash \u2014 API Overview",
  "panels": [
    {
      "type": "graph",
      "title": "Requests rate (rps)",
      "targets": [
        {
          "expr": "sum(rate(http_requests_total[5m]))"
        }
      ]
    },
    {
      "type": "graph",
      "title": "5xx error rate (%)",
      "targets": [
        {
          "expr": "100 * sum(rate(http_requests_total{code=~\"5..\"}[5m])) / sum(rate(http_requests_total[5m]))"
        }
      ]
    },
    {
      "type": "graph",
      "title": "Latency p95 (s)",
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum by (le) (rate(http_request_duration_seconds_bucket[5m])))"
        }
      ]
    }
  ],
  "schemaVersion": 36
}
